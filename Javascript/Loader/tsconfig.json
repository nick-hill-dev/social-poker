{
    "compileOnSave": true,
    "compilerOptions": {
      "target": "es5",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "module": "system",
      "removeComments": false,
      "sourceRoot": "./src/",
      "rootDir": "./src/",
      "outDir": "./bin/",
      "outFile": "./bin/loader.js",
      "noEmitOnError": true,
      "composite": true,
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true
    }
  }